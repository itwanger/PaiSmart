    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@iconify-json/carbon": "^1.2.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@types/spark-md5": "^3.0.5",
